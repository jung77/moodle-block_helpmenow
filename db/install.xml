        <FIELD NAME="cache" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="optimistic_last_message" NEXT="last_read"/>
        <FIELD NAME="last_read" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="id for message from mdl_block_helpmenow_message of the last message that the user has actively seen (no unfocused messages)" PREVIOUS="cache"/>
